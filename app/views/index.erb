<div class="container">
  <h1>User Skills and Levels</h1>

  <table border="1">
<tr>
<th>Name</th>
<th>Skill</th>
<th>Year</th>
<th>Formal</th>
</tr>

  <% @users.each do |user| %>
<tr>
<td><%= user.name%></td>
<td><%= user.skills.first.name%></td>
<td><%= user.skills.first.proficiencies.first.years%></td>
<td><%= user.skills.first.proficiencies.first.formal%></td>
</tr>
<% end %>
</table>

  <%# TODO: display the skills and proficiency levels for each user ... %>
</div>
